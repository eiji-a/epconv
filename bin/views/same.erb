<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>SAME MAG</title>
  <style type="text/css">
    <!--
    body {
    font-family: Avenir;
    font-size: 10pt;
    }
    -->
  </style>
</head>
<body>
  SAME: <%= @sames.size %>

  <table border="0">
  <% @sames.each do |s| %>
    <tr>
      <td>
        <% TYPE.each_key do |k| %>
        <%   if k == s[0][3] %>
        <%=    TYPE[k] %>
        <%   else %>
        <a href="/change/<%= @st %>/<%= @pg %>/<%= @filter %>/<%= s[0][0] %>/0000/<%= k %>">
          <%= TYPE[k] %>
        </a>
        <%   end %>
        <% end %>
        <br />
        size: <%= s[0][2] %> / #img: <%= s[0][1].size %><br />
        <a href="/mag/n/0/all/<%= s[0][0] %>/0" target="s0">
          <img src="/index/emags-<%= s[0][0] %>-index.jpg" width="200px">
        </a>
      </td>
      <td>
        <% TYPE.each_key do |k| %>
        <%   if k == s[1][3] %>
        <%=    TYPE[k] %>
        <%   else %>
        <a href="/change/<%= @st %>/<%= @pg %>/<%= @filter %>/<%= s[1][0] %>/0000/<%= k %>">
          <%= TYPE[k] %>
        </a>
        <%   end %>
        <% end %>
        <br />
        size: <%= s[1][2] %> / #img: <%= s[1][1].size %><br />
        <a href="/mag/n/0/all/<%= s[1][0] %>/0" target="s1">
          <img src="/index/emags-<%= s[1][0] %>-index.jpg" width="200px">
        </a>
      </td>
      <td>
        SAME IMAGES:
          <a href="/samedtl/<%= s[0][0] %>/<%= s[1][0] %>">
            <%= s[2].size %>
          </a>
      </td>
    </tr>
  <% end %>
  </table>
  
</body>
</html>

